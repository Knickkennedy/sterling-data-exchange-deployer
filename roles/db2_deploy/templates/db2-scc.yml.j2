kind: SecurityContextConstraints
metadata:
  name: db2-scc
allowHostPorts: false
priority: 10
requiredDropCapabilities:
  - ALL
allowPrivilegedContainer: true
runAsUser:
  type: RunAsAny
users:
  - system:serviceaccount:db2-tests:{{ db2_namespace }}:{{ db2_service_account }}
allowHostDirVolumePlugin: true
seccompProfiles:
  - unconfined
  - runtime/default
allowHostIPC: false
seLinuxContext:
  type: MustRunAs
readOnlyRootFilesystem: false
fsGroup:
  type: RunAsAny
groups: []
kind: SecurityContextConstraints
defaultAddCapabilities: null
supplementalGroups:
  type: RunAsAny
volumes:
  - '*'
allowHostPID: false
allowHostNetwork: false
allowPrivilegeEscalation: true
apiVersion: security.openshift.io/v1
allowedCapabilities:
  - CHOWN
  - DAC_OVERRIDE
  - SETGID
  - SETUID
  - FOWNER
  - SYS_RESOURCE
  - IPC_OWNER
  - SYS_NICE
  - FSETID
  - SETFCAP
  - SETPCAP
  - SYS_CHROOT
  - KILL
  - AUDIT_WRITE
