---
kind: RoleBinding
apiVersion: rbac.authorization.k8s.io/v1
metadata:
  name: "privileged"
  namespace: "{{ mq_namespace }}"
  labels:
    app: "privileged"
roleRef:
  kind: ClusterRole
  name: "system:openshift:scc:privileged"
  apiGroup: rbac.authorization.k8s.io
subjects:
  - kind: ServiceAccount
    name: qmgr-ibm-mq
    namespace: {{ mq_namespace }}